{
  "name": "resume-express",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "express": "^4.16.3",
    "nodemailer": "^4.6.4",
    "nodemon": "^1.17.3",
    "npm": "^5.8.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
